<h1>Created events</h1>
<%= render @events %>

<h1>Past events:</h1>
<ul>
  <% current_user.attended_events.past.each do |event| %>
    <li><%= event %></li>
  <% end %>
</ul>

<h1>Upcoming events:</h1>
<ul>
  <% current_user.attended_events.upcoming.each do |event| %>
    <li><%= event %></li>
  <% end %>
</ul>
